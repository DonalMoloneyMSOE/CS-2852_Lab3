<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Moloneyda.DToDController">
    <children>
        <MenuBar prefHeight="39.0" prefWidth="600.0">
            <menus>
                <Menu fx:id="menuFile" mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="menuItemOpen" mnemonicParsing="false" onAction="#openFile" text="Open" />
                        <MenuItem fx:id="menuItemClose" mnemonicParsing="false" onAction="#closeFile" text="Close" />
                        <MenuItem fx:id="menuItemSave" disable="true" mnemonicParsing="false" onAction="#save" text="Save" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Draw">
                    <items>
                        <MenuItem fx:id="menuItemLinesOnly" disable="true" mnemonicParsing="false" onAction="#drawLinesOnly" text="Lines Only" />
                        <MenuItem fx:id="menuItemDotsOnly" disable="true" mnemonicParsing="false" onAction="#drawDotsOnly" text="Dots Only" />
                        <MenuItem fx:id="menuItemRemoveDots" disable="true" mnemonicParsing="false" onAction="#removeDots" text="Remove Dots" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Canvas fx:id="canvas" height="650" layoutX="14.0" layoutY="51.0" width="570.0" />
    </children>
</Pane>
